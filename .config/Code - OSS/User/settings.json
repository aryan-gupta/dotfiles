{
  "terminal.integrated.rendererType": "dom",
  "workbench.colorCustomizations": {
    "workbench.background": "#1A1A1AFF",

    "activityBar.background": "#1A1A1AFF",
    "button.background": "#6d04aeff",
    "button.hoverBackground": "#AF45DCFF",

    "editor.background": "#1A1A1AFF",
    "editor.lineHighlightBackground": "#2A2A2AFF",
    "editorGroupHeader.tabsBackground": "#1A1A1AFF",
    "tab.activeBackground": "#1A1A1AFF",
    "tab.inactiveBackground": "#0A0A0AFF",
    "tab.border": "#1A1A1AFF",

    "list.activeSelectionBackground": "#ffffff00",
    "list.hoverBackground": "#ffffff00",
    "list.inactiveSelectionBackground": "#ffffff00",

    "panel.background": "#ffffff00",
    "panel.border": "#ffffff00",

    "sideBar.background": "#1A1A1AFF",
    "sideBarSectionHeader.background": "#0A0A0AFF",

    "statusBar.background": "#626D61FF",
    "statusBar.noFolderBackground": "#626D61FF",
    "statusBar.debuggingBackground": "#626D61FF",
    "terminal.background": "#1A1A1AFF",
    "titleBar.activeBackground": "#db0b0bFF",

    "menu.background": "#db0b0b",
    "menubar.selectionForeground": "#db0b0b",
    "menubar.selectionBackground": "#db0b0b",
    "menu.border": "#db0b0b"
  },
  "workbench.sideBar.location": "right",
  "editor.minimap.enabled": false,
  "window.zoomLevel": -1,
  "window.menuBarVisibility": "compact",
  "files.associations": {
    "*.rasi": "css"
  },
  "editor.largeFileOptimizations": false,
  "security.workspace.trust.untrustedFiles": "open",
  "redhat.telemetry.enabled": false,
  "workbench.colorTheme": "Default Dark Modern",
}
